      }
    },
    "base": {
      "mainnet": {
        "url": "https://api.basescan.org/api",
        "apiKey": "${BASESCAN_API_KEY}"
      },
      "sepolia": {
        "url": "https://api-sepolia.basescan.org/api",
        "apiKey": "${BASESCAN_API_KEY}"
      }
    }
  }
