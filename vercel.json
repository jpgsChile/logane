{
  "functions": {
    "app/api/og/route.tsx": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/frame",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    },
    {
      "source": "/farcaster.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/farcaster.json",
      "destination": "/api/farcaster-config"
    }
  ]
}
